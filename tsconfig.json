{
  "extends": "./tsconfig.build.json",
  "module": "commonjs",
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "target": "es6",
  "noImplicitAny": true,
  "moduleResolution": "node",
  "sourceMap": true,
  "compilerOptions": {
    "baseUrl": "./"
  },
  "include": [
    "./server/**.ts",
    "./core/**.ts",
    "./app/**.ts"
  ]
}